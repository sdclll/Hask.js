setitle("二进制转十进制"); 
se(1,gea(1));   //获取用户输入并赋值给全局变量组id为1的元素
se(2,obj[1].split("")); //将用户输入转化成数组，赋值给全局变量组id为2的元素
se(3,obj[2].length-1);  //将id为3的全局变量赋值成id为2的全局变量（id为1的全局变量所转化的数组）的长度减去1
se(4,new Array(obj[2]));    //将id为4的全局变量赋值为数组，长度为全局变量id为2的元素
se(5,0);    //将id为5的全局变量赋值为0
se(6,0);    //将id为6的全局变量赋值为0
exmod(readmod("zh.modhsj"));    //将模块zh.modhsj执行，此模块是将用户的输入进行进一步处理
isfor("se(6,i);isif(obj[4][i]==1,'se(5,obj[5]+Math.pow(2,obj[6]));','');",obj[2].length); //二进制转十进制算法
putxt(obj[5],1);    //输出结果